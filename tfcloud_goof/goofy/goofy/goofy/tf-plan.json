at_version":"0.1","terraform_version":"0.14.9","values":{"root_module":{"resources":[{"address":"data.template_file.userdata_setup","mode":"data","type":"template_file","name":"userdata_setup","provider_name":"registry.terraform.io/hashicorp/template","schema_version":0,"values":{"filename":null,"id":"55a70e6170a27a6eb626155eef75d7dc8257a74758b10e6973841ce463408612","rendered":"#!/bin/bash\n# Set Variables we need in the userdata logic\n# Output the logic that we will use for UserData or CustomData\n#!/bin/bash\n\n# Update and download Nginx\nuntil sudo apt-get update \u0026\u0026 sudo apt-get -y install nginx;do\n    sleep 1\ndone\n\n# Update and download docker\nuntil sudo  apt-get -y install docker.io;do\n    sleep 1\ndone\n\n# Pull Juice Repository\nuntil sudo docker pull bkimminich/juice-shop;do\n    sleep 1\ndone\n\n# Start Docker\nsudo service nginx stop\nsudo docker run --rm -d -p 3000:3000 bkimminich/juice-shop:latest\n\n# Gather IP address\n\nip_address=$(dig +short myip.opendns.com @resolver1.opendns.com)\n\n# Modify Nginx Config to allow access to Juice Store\nsudo touch /etc/nginx/conf.d/juice.conf\n\nsudo chmod a+w /etc/nginx/conf.d/juice.conf\n\nsudo cat \u003c\u003cEOT \u003e\u003e /etc/nginx/conf.d/juice.conf\nserver {\n  listen 80;\n  listen [::]:80;\n\n  server_name $ip_address;\n\n  location / {\n      proxy_pass http://localhost:3000/;\n  }\n}\nEOT\n\nsudo service nginx restart\nsleep 2\nsudo nginx -s reload\n\ndir=home/ubuntu/\n\n","template":"#!/bin/bash\n# Set Variables we need in the userdata logic\n# Output the logic that we will use for UserData or CustomData\n${logic}","vars":{"logic":"#!/bin/bash\n\n# Update and download Nginx\nuntil sudo apt-get update \u0026\u0026 sudo apt-get -y install nginx;do\n    sleep 1\ndone\n\n# Update and download docker\nuntil sudo  apt-get -y install docker.io;do\n    sleep 1\ndone\n\n# Pull Juice Repository\nuntil sudo docker pull bkimminich/juice-shop;do\n    sleep 1\ndone\n\n# Start Docker\nsudo service nginx stop\nsudo docker run --rm -d -p 3000:3000 bkimminich/juice-shop:latest\n\n# Gather IP address\n\nip_address=$(dig +short myip.opendns.com @resolver1.opendns.com)\n\n# Modify Nginx Config to allow access to Juice Store\nsudo touch /etc/nginx/conf.d/juice.conf\n\nsudo chmod a+w /etc/nginx/conf.d/juice.conf\n\nsudo cat \u003c\u003cEOT \u003e\u003e /etc/nginx/conf.d/juice.conf\nserver {\n  listen 80;\n  listen [::]:80;\n\n  server_name $ip_address;\n\n  location / {\n      proxy_pass http://localhost:3000/;\n  }\n}\nEOT\n\nsudo service nginx restart\nsleep 2\nsudo nginx -s reload\n\ndir=home/ubuntu/\n\n"}}}]}}},"configuration":{"provider_config":{"aws":{"name":"aws","expressions":{"region":{"references":["var.aws_region"]}}}},"root_module":{"outputs":{"public_ip":{"expression":{"references":["aws_instance.vuln_vm"]},"description":"List of public IP addresses assigned to the instances, if applicable"}},"resources":[{"address":"aws_eip.goof_eip","mode":"managed","type":"aws_eip","name":"goof_eip","provider_config_key":"aws","expressions":{"associate_with_private_ip":{"references":["var.goof_private"]},"network_interface":{"references":["aws_network_interface.goof-nic"]},"vpc":{"constant_value":true}},"schema_version":0,"depends_on":["aws_internet_gateway.goof_demo_gateway"]},{"address":"aws_instance.vuln_vm","mode":"managed","type":"aws_instance","name":"vuln_vm","provider_config_key":"aws","expressions":{"ami":{"references":["var.ubuntu_ami"]},"availability_zone":{"references":["var.primary_az"]},"instance_type":{"constant_value":"t2.micro"},"key_name":{"references":["var.key_name"]},"network_interface":[{"device_index":{"constant_value":0},"network_interface_id":{"references":["aws_network_interface.goof-nic"]}}],"tags":{"constant_value":{"Environment":"dev"}},"user_data":{"references":["data.template_file.userdata_setup"]}},"schema_version":1},{"address":"aws_internet_gateway.goof_demo_gateway","mode":"managed","type":"aws_internet_gateway","name":"goof_demo_gate